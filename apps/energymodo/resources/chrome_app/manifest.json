{
  "name": "EnergyModo Energy-Tracking App",
  "description": "Track and see what affects your energy levels!",
  "version": "IONIC_APP_VERSION_NUMBER_PLACEHOLDER",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "icons": {
      "16" : "www/img/icons/icon_16.png",
      "48" : "www/img/icons/icon_48.png",
      "128" : "www/img/icons/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["www/js/qmHelpers.js"]
    }
  },
  "permissions": [
    "identity",
    "geolocation",
    "storage",
    "notifications",
    "alarms",

    "http://api.curedao.org/*"
  ]
}
