PHP_IDE_CONFIG=serverName=dockerCompose
APPLICATION_SSH_HOST_PORT=2222

SAIL_XDEBUG_MODE=develop,debug
#SAIL_XDEBUG_CONFIG="client_host=localhost"
SLACK_WEBHOOK_URL=
SLACK_OTHER_WEBHOOK_URL=

